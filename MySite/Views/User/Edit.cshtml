@model MySite.Models.User
@{
    Layout = "_Layout";
}
<h2>Edit: @Model.UserName</h2>
<form asp-action="Edit" method="post" class="form-floating mb-3">
    <input type="hidden" asp-for="@Model.Id"></input>
    
     <label for="floatingInputValue2" class="form-lable"></label>

    <input type="url" class="form-control" id="floatingInputValue2"  asp-for="@Model.Urls"  value="@ViewBag.LinkEdit">
       
    <input type="submit" class="btn btn-primary" asp-route-Url="@ViewBag.LinkEdit" value="Update"></input>
</form>